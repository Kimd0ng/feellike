{
    "name": "@feellike/mobile",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "build:android": "cd android && ./gradlew assembleRelease",
        "build:ios": "cd ios && xcodebuild -workspace FeelLikeMobile.xcworkspace -scheme FeelLikeMobile -configuration Release",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit",
        "clean": "cd android && ./gradlew clean && cd ../ios && xcodebuild clean"
    },
    "dependencies": {
        "@feellike/api": "workspace:*",
        "@feellike/ui": "workspace:*",
        "@react-native-async-storage/async-storage": "^2.1.0",
        "@react-navigation/native": "^7.0.14",
        "@react-navigation/native-stack": "^7.2.0",
        "@supabase/supabase-js": "^2.48.0",
        "@tanstack/react-query": "^5.83.0",
        "jotai": "^2.13.0",
        "react": "18.3.1",
        "react-native": "0.76.5",
        "react-native-config": "^1.6.1",
        "react-native-geolocation-service": "^5.3.1",
        "react-native-inappbrowser-reborn": "^3.7.0",
        "react-native-safe-area-context": "^4.14.0",
        "react-native-screens": "^4.4.0",
        "react-native-url-polyfill": "^2.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/runtime": "^7.26.0",
        "@feellike/eslint-config": "workspace:*",
        "@feellike/typescript-config": "workspace:*",
        "@react-native/babel-preset": "0.76.5",
        "@react-native/eslint-config": "0.76.5",
        "@react-native/gradle-plugin": "0.76.5",
        "@react-native/metro-config": "0.76.5",
        "@react-native/typescript-config": "0.76.5",
        "@types/react": "^18.3.0",
        "@types/react-native": "^0.73.0",
        "eslint": "^9.17.0",
        "metro-react-native-babel-preset": "^0.77.0",
        "typescript": "~5.5.4"
    },
    "engines": {
        "node": ">=18"
    }
}
