{
    "name": "@feellike/ui",
    "version": "0.0.0",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/feellike-ui.es.js",
            "require": "./dist/feellike-ui.umd.js",
            "types": "./dist/index.d.ts"
        },
        "./style.css": "./dist/style.css",
        "./native": {
            "import": "./src/native/index.ts",
            "require": "./src/native/index.ts",
            "types": "./src/native/index.ts"
        },
        "./shared": {
            "import": "./src/shared/index.ts",
            "require": "./src/shared/index.ts",
            "types": "./src/shared/index.ts"
        }
    },
    "main": "./dist/feellike-ui.umd.js",
    "module": "./dist/feellike-ui.es.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src/native",
        "src/shared"
    ],
    "scripts": {
        "build": "vite build",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@vanilla-extract/recipes": "catalog:",
        "@radix-ui/react-dialog": "^1.1.14"
    },
    "peerDependencies": {
        "@vanilla-extract/css": "catalog:",
        "react": "catalog:",
        "react-dom": "catalog:",
        "react-native": ">=0.70.0"
    },
    "peerDependenciesMeta": {
        "react-dom": {
            "optional": true
        },
        "react-native": {
            "optional": true
        },
        "@vanilla-extract/css": {
            "optional": true
        }
    },
    "devDependencies": {
        "@feellike/typescript-config": "workspace:*",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.1",
        "@vanilla-extract/css": "catalog:",
        "@vanilla-extract/vite-plugin": "catalog:",
        "@vitejs/plugin-react": "catalog:",
        "react": "catalog:",
        "react-dom": "catalog:",
        "typescript": "catalog:",
        "vite": "catalog:",
        "vite-plugin-dts": "catalog:"
    }
}
